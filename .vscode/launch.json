{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug crimson-watch",
        "type": "debugpy",
        "request": "launch",
        "module": "crimson_watch",
        "args": ["status"],
        "justMyCode": true,
        "console": "integratedTerminal"
      },
      {
        "name": "Attach to CLI",
        "type": "debugpy",
        "request": "attach",
        "connect": {
          "host": "localhost",
          "port": 5678
        },
        "justMyCode": true
      }
    ]
  }
  